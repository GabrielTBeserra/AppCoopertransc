{
  "expo": {
    "name": "AppCoopertransc",
    "slug": "AppCoopertransc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-fix.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo-coper.png",
      "resizeMode": "contain",
      "backgroundColor": "#044f4b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-coper.png",
        "backgroundColor": "#044f4b"
      },
      "package": "com.gabrielteles.AppCoopertransc"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}